<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Camera"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <title
            jcr:primaryType="nt:unstructured"
            defaultValue="Camera"
            fieldLabel="Title"
            name="./title"
            xtype="textfield"/>
        <subtitle
            jcr:primaryType="nt:unstructured"
            defaultValue="Take a picture"
            fieldLabel="Subtitle"
            name="./subtitle"
            xtype="textfield"/>
        <camera
            jcr:primaryType="nt:unstructured"
            defaultValue="Take a picture"
            fieldLabel="Camera prompt"
            name="./useCamera"
            xtype="textfield"/>
        <browse
            jcr:primaryType="nt:unstructured"
            defaultValue="Browse gallery"
            fieldLabel="Browse prompt"
            name="./useGallery"
            xtype="textfield"/>
    </items>
</jcr:root>
